            <declare name="G" type="Integer" array="False" size=""/>
            <declare name="M" type="Integer" array="False" size=""/>
            <declare name="X" type="Integer" array="False" size=""/>
            <output expression="&quot;Enter gallons used&quot;" newline="True"/>
