        <body>
            <declare name="no1" type="Real" array="False" size=""/>
            <declare name="no2" type="Real" array="False" size=""/>
            <declare name="s" type="Real" array="False" size=""/>
            <declare name="p" type="Real" array="False" size=""/>
            <declare name="d" type="Real" array="False" size=""/>
            <output expression="&quot;Enter the first number&quot;" newline="True"/>
            <input variable="no1"/>
            <output expression="&quot;Enter another number&quot;" newline="True"/>
            <input variable="no2"/>
            <assign variable="s" expression="no1 + no2"/>
            <assign variable="p" expression="no1 * no2"/>
            <assign variable="d" expression="no1 - no2"/>
